{
  "name": "notes-app",
  "private":true,
  "version": "1.0.0",
  "description": "",
  "workspaces":[
    "Fronted",
    "Backend"
  ],
  "engines":{
    "npm":"7.9.0"
  },
  "main": "index.js",
  "scripts": {
    "build":"SKIP_PREFLIGHT_CHECK=true npm run build --workspace=Fronted",
    "start":"npm run start --workspace=Backend",
    "dev:api":"npm run build && npm run dev --workspace=Backend",
    "lint":"eslint ./Fronted ./Backend"
  },
  "eslintConfig": {
    "ignorePatterns": "build",
    "extends": "./node_modules/standard/eslintrc.json",
    "env": {
      "jest": true,
      "cypress/globals":true
    },
    "plugins": [
      "cypress"
    ]
  },
  "devDependencies": {
    "eslint-plugin-cypress": "^2.12.1",
    "standard": "16.0.4"
  },
  "keywords": [],
  "author": "",
  "license": "ISC"
  

}
